#Ant Installer - AutoGenerated properties
ant.install.config.version=0.0

# Version
antinstaller_versione_attuale_con_p_level=#GOVWAY_FULLVERSION#
antinstaller_versione_attuale=#GOVWAY_FULLVERSION#
antinstaller_versione_precedente=

# Modalità Installazione
antinstaller_conf_avanzata_enabled=
antinstaller_modalita_installazione=nuova
