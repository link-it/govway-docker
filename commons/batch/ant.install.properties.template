# Informazioni Dispiegamento
antinstaller_config_db=config
antinstaller_log_db=log
antinstaller_index_transazioni=full
antinstaller_stats_db=stats
antinstaller_index_statistiche=full

# Tools
antinstaller_tools_cliVault=true
